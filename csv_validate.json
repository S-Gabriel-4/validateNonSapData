{
  "id": "com.example.csvvalidate",
  "name": "CSV Validation",
  "description": "Reads a CSV locally and counts rows (excluding header).",
  "version": "1.0.1",
  "vendor": "You",
  "newInstancePrefix": "CSVValidation",

  "properties": {
    "rowCount": { "type": "number", "default": 0 },
    "fileName": { "type": "string", "default": "" }
  },

  "methods": {
    "getRowCount":           { "returnType": "number" },
    "getFileName":           { "returnType": "string" },
    "getDuplicateCount":     { "returnType": "number" },
    "getDuplicatePairsJson": { "returnType": "string" }
  },

  "events": {
    "fileLoaded": {
      "description": "Fires after a CSV parsed; updates rowCount."
    },
    "duplicatesFound": {
      "description": "Fires after parsing; provides duplicate info."
    }
  },

  "webcomponents": [
    {
      "kind": "main",
      "tag": "csv-validation",
      "url": "https://s-gabriel-4.github.io/validateNonSapData/main.js",
      "integrity": "sha256-l1ci2RpCVdkbuSsBNs24Qg5ZQsnvR1hGKLE8j+JDLF0="
    }
  ]
}
